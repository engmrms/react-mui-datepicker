{
  "name": "react-mui-datepicker",
  "version": "2.0.0",
  "description": "A powerful, flexible, and customizable date picker component that supports both Gregorian and Hijri (Umm Al-Qura) calendars",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "build:analyze": "rollup -c --bundleConfigAsCjs --analyze",
    "start": "rollup -c -w",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "typescript",
    "mui",
    "datepicker",
    "hijri",
    "gregorian",
    "calendar",
    "umm-al-qura",
    "islamic",
    "accessible",
    "internationalization"
  ],
  "author": "Mohammad Saleh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/engmrms/react-mui-datepicker"
  },
  "homepage": "https://github.com/engmrms/react-mui-datepicker",
  "bugs": {
    "url": "https://github.com/engmrms/react-mui-datepicker/issues"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.3",
    "@storybook/react": "^7.2.3",
    "@storybook/react-vite": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sass": "^1.12.20",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "storybook": "^7.2.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@date-io/moment": "^2.17.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^6.18.7",
    "@types/moment-hijri": "^2.1.2",
    "moment": "^2.29.4",
    "moment-hijri": "^2.1.2",
    "radix-ui": "^1.4.3"
  },
  "files": [
    "dist",
    "README.md",
    "ENHANCED_README.md",
    "CUSTOM_ELEMENTS_README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
